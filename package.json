{
  "name": "vite-tailwind-starter",
  "description": "NoJS Vite + Tailwind CSS Starter",
  "version": "0.0.0",
  "keywords": [
    "template",
    "scaffold",
    "tailwind",
    "tailwindcss",
    "tailwind-css",
    "vite",
    "vitejs",
    "vite-template",
    "vitejs-template",
    "vite-tailwind"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "css-build": "npx tailwindcss -i ./src/style.css -o ./src/style-output.css",
    "css-watch": "npx tailwindcss -i ./src/style.css -o ./src/style-output.css --watch",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^10.4.13",
    "color": "^4.2.3",
    "cssnano": "^5.1.14",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.2.3",
    "tailwindcss-multi-theme": "^1.0.4",
    "vite": "^4.1.3"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13"
  }
}
